vowel = ['a','e','i','o','u']
count = 0
v = ''
word_str = ''

def changer(v):
    word_ar.pop(i)
    word_ar.insert(i,'haha')
    return v

word = input("Pleae input a word:")
word_ar = list(word)

for i in range (0,len(word_ar)):
    for f in range (0,5):
        if vowel[f] in word_ar[i]:
            word_str = changer(v)

vowel_final = ''.join(word_ar)
print(vowel_final)
